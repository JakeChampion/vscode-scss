{
	"compilerOptions": {
		"target": "es2015",
		"module": "commonjs",
		"moduleResolution": "node",
		"rootDir": "src",
		"outDir": "out",
		"forceConsistentCasingInFileNames": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"pretty": true,
		"declaration": true,
		"sourceMap": true
	}
}
