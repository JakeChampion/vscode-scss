{
	"compilerOptions": {
		"target": "es2017",
		"module": "commonjs",
		"moduleResolution": "node",
		"rootDir": "src/unsafe",
		"outDir": "out/unsafe",
		"forceConsistentCasingInFileNames": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"pretty": true,
		"declaration": true,
		"sourceMap": true
	},
	"include": [
		"src/unsafe/**/*"
	]
}
